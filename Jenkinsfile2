#!/usr/bin/env groovy

@Library('shared-library@master') _ //master or whatever branch

pipeline{

	agent any
	
	
        
        stages{

	      stage('Git Checkout'){
                   steps{
                        git credentialsId: 'git_ssh', url: 'https://github.com/Navya-Danvi/sample-web-application.git'
                   }
              }
		
              stage('maven build'){
                  steps{
                      script{
		    	                sh "mvn clean install"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 50)
                    }
                }
		
           }	       	     	         
}
