#!/usr/bin/env groovy

@Library('shared-library@master') _ //master or whatever branch

pipeline{

	agent any
	
	tools {
        jdk 'OPENJDK8'
        maven 'MAVEN_3.6.0'
        }
        
        stages{

              stage('maven build'){
                  steps{
                      script{
		    	                sh "mvn clean install"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 50)
                    }
                }
		
           }	       	     	         
}
